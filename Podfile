project 'BBS.xcodeproj'

# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'BBS' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
   use_frameworks!
   pod 'AFNetworking', '~> 3.2.1'
   pod 'YYKit', :git => 'https://gitee.com/zhuzhuxingtianxia/YYKit.git'
   pod 'SVPullToRefresh'
   
  # Pods for BBS

  target 'BBSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BBSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
